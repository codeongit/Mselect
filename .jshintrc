{
  "browser": true,
  "es3": true,
  "strict": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "eqnull": false,
  "evil": false,
  "forin": true,
  "funcscope": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "maxerr": 7,
  "newcap": true,
  "nonew": true,
  "quotmark": "single",
  "shadow": true,
  "trailing": true,
  "undef": true,
  "unused": true,
  "maxparams": 5,
  "maxdepth": 5,
  "maxstatements": 25,
  "maxcomplexity": 10
}
